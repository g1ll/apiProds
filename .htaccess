<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /2019/tsi/dsw/apiProds/
  RewriteCond $1 !^(index\.php|images|assets|robots\.txt|bootstrap.min.css\.map)
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^(.*)$ index.php?query=$1 [L]
  php_value error_log all_errors.log
</IfModule>

<IfModule !mod_rewrite.c>
  ErrorDocument 404 /2019/tsi/dsw/apiProds/view/404.html
  ErrorDocument 403 /2019/tsi/dsw/apiProds/view/404.html
</IfModule>
